{
  "rules": {
    "leaderboards": {
      "$challengeId": {
        "$date": {
          ".read": true,
          ".write": true,
          "$attemptId": {
            ".validate": "newData.hasChildren(['userId', 'timeMs', 'displayName', 'completedAt'])"
          }
        }
      }
    },
    "pots": {
      "$challengeId": {
        "$date": {
          ".read": true,
          ".write": true
        }
      }
    }
  }
}